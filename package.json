{
  "name": "whattowatch",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android:build": "eas build --platform android",
    "android:dev": "eas build --profile development-simulator --platform android",
    "android:local": "eas build --profile preview --platform android --local",
    "android:preview": "eas build --profile preview --platform android",
    "android": "expo run:android",
    "dev": "expo start --dev-client",
    "eject": "expo eject",
    "generate:api:types": "npx openapi-typescript https://developer.themoviedb.org/openapi/64542913e1f86100738e227f -o ./src/api/types.d.ts && npx openapi-typescript https://developer.themoviedb.org/openapi/6453cc549c91cf004cd2a015 -o ./src/api/types-v4.d.ts",
    "generate:icons": "npx @svgr/cli -- src/components/Icon/icons && yarn eslint src/components/Icon --max-warnings 0 --fix",
    "ios:build": "eas build --platform ios",
    "ios:dev": "eas build --profile development-simulator --platform ios",
    "ios:local": "eas build --profile preview --platform ios --local",
    "ios:preview": "eas build --profile preview --platform ios",
    "ios": "expo run:ios",
    "lint:js": "eslint --max-warnings 0",
    "lint:ts": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:ts",
    "pre-release": "eas update --branch pre-release --message",
    "prepare": "husky",
    "preview": "eas update --branch preview --message",
    "start": "expo start",
    "test:ci": "jest --coverage=false --forceExit",
    "test:coverage": "jest",
    "test:e2e": "rm -rf maestro/screenshots && maestro test maestro/",
    "test": "jest --watchAll --coverage=false",
    "translate:extract": "yarn formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/locales/temp.json --flatten --format simple",
    "translate": "yarn translate:extract && node ./scripts/translation.js",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.3.1",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.4.0",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.3.0",
    "@shopify/flash-list": "^2.0.0",
    "@tanstack/react-query": "^5.75.2",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "expo": "^54.0.0",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.10",
    "intl": "^1.2.5",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "^7.1.11",
    "react-is": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-youtube-iframe": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@formatjs/cli": "^6.6.4",
    "@react-native/babel-preset": "^0.80.2",
    "@svgr/cli": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^24.1.0",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.32.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-formatjs": "^5.3.1",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.11",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "private": true,
  "resolutions": {
    "strip-ansi": "^6.0.1",
    "string-width": "^4.2.3",
    "wrap-ansi": "^7.0.0"
  }
}
